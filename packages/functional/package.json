{
  "version": "0.0.1",
  "name": "functional",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "tsnode": "ts-node ./src/index.ts",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "sonar-scanner": "^3.1.0",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|svg|SVG|scss|SCSS|sass|SASS|less|LESS|css|CSS)$": "tests/empty_module"
     },
    "modulePaths" : ["./"],
    "bail": true,
    "verbose": false,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom" : ["src/**/*.ts"],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 60
      }
    }
  }
}
